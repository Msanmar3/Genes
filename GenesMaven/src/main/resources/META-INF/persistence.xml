<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="genes_PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.upo.genesmaven.entities.RolesMenus</class>
    <class>com.upo.genesmaven.entities.Menus</class>
    <class>com.upo.genesmaven.entities.Users</class>
    <class>com.upo.genesmaven.entities.Roles</class>
    <class>com.upo.genesmaven.entities.Files</class>
    <class>com.upo.genesmaven.entities.Nomenclatures</class>
    <class>com.upo.genesmaven.entities.Iterations</class>
    <class>com.upo.genesmaven.entities.Authors</class>
    <class>com.upo.genesmaven.entities.Species</class>
    <class>com.upo.genesmaven.entities.IterationsSpecies</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://127.0.0.1:3300/genevalidator?serverTimezone=UTC"/>
      <property name="javax.persistence.jdbc.user" value="genes"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="genes"/>
    </properties>
  </persistence-unit>
</persistence>
